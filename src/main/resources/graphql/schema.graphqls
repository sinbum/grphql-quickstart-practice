
type Query {
bookById(id: ID): Book
books: [Book]

}

type Book {
id: ID
name: String
pageCount: Int
author: Author
}

type Author {
id: ID
firstName: String
lastName: String
}

type Subscription {
    book: Book
}

type Mutation {
    addBook(id: ID!, name: String, pageCount: Int, authorId: ID): Book
}